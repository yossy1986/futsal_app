<main>
<%= render partial: 'layouts/advertising' %>
<div class="wrapper roomss new">
  <div class="left">
    <h2>記事作成</h2>
    <div class="message">
      <%= form_for @message do |f| %>
      <%= f.label :content,"内容" %><br>
      <%= f.text_area :content, value: @message.content %><br>
      <%= f.submit "編集する",:class => "btn btn-info" %>
      <% end %>
    </div>
  </div>
  <%= render partial: 'posts/post' %>
</div>
</main>